# GNU Make workspace makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  cpim2_config = debug
endif
ifeq ($(config),release)
  cpim2_config = release
endif

PROJECTS := cpim2

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

cpim2:
ifneq (,$(cpim2_config))
	@echo "==== Building cpim2 ($(cpim2_config)) ===="
	@${MAKE} --no-print-directory -C . -f cpim2.make config=$(cpim2_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f cpim2.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   cpim2"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"